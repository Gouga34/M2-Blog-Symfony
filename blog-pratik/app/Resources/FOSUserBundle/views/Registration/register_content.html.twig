
<h1> Inscription </h1>  

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal" role="form">
 
  {{ form_errors(form.username) }}  
  
  {{ form_errors(form.plainPassword.first) }}
  
  {{ form_errors(form.plainPassword.second) }}
  
  {{ form_errors(form.email) }}


  {{ form_rest(form) }}
 
<button type="submit" class="btn btn-primary">Inscription <span class="glyphicon glyphicon-ok"></span></button>
