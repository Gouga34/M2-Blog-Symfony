<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal">
     
    <div class="control-group">
        {{ form_label(form.username, 'security.login.username'|trans({}, 'FOSUserBundle'), { 'attr': {'class': 'control-label', 'autocomplete': 'off', 'value': ' '} }) }}
        <div class="controls">
            <div class="input-prepend">
                <span class="add-on"><i class="icon-user"></i></span>{{ form_widget(form.username) }}
            </div>
        </div>
    </div>
 
    <div class="control-group">
        {{ form_label(form.email,  'fos_user_registration_form_email'|trans({}, 'FOSUserBundle'), { 'attr': { 'class': 'control-label', 'autocomplete': 'off', 'value': ' ' } }) }}
        <div class="controls">
            <div class="input-prepend">
                <span class="add-on">@</span>{{ form_widget(form.email) }}
            </div>
        </div>
    </div>
 
    <div class="control-group">
        {{ form_label(form.plainPassword.first,  'security.login.password'|trans({}, 'FOSUserBundle'), { 'attr': { 'class': 'control-label', 'autocomplete': 'off', 'value': ' ' } }) }}
        <div class="controls">
            <div class="input-prepend">
                <span class="add-on"><i class="icon-lock"></i></span>{{ form_widget(form.plainPassword.first) }}
            </div>
        </div>
    </div>
     
    <div class="control-group">
        {{ form_label(form.plainPassword.second,  'fos_user_registration_form_plainPassword_second'|trans({}, 'FOSUserBundle'), { 'attr': { 'class': 'control-label', 'autocomplete': 'off', 'value': ' ' } }) }}
        <div class="controls">
            <div class="input-prepend">
                <span class="add-on"><i class="icon-lock"></i></span>{{ form_widget(form.plainPassword.second) }}
            </div>
        </div>
    </div>
    {{ form_rest(form) }}       
    <div>
        <input class="btn btn-primary" type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>